EspTAllowDuplicates:
name        Erlaube Mehrfachbuchungen
description Schüler können mehrere Termine bei einer Gruppe buchen.
type        bool
group	    Modul: Elternsprechtag
default	    false

EspTAllowOverlap:
name        Erlaube zeitliche Überlappung
description Schüler können Termine buchen, auch wenn sich diese überlappen
type        bool
group	    Modul: Elternsprechtag
default	    false

EspTAllowEditByTeacher:
name        Erlaube Bearbeitung durch Lehrkräfte
description Lehrkräfte können ihre eigenen Termine bearbeiten
type        bool
group	    Modul: Elternsprechtag
default	    false