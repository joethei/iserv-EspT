{%  extends "@IServCore/page.html.twig" %}

{% block stylesheets %}

    {% if app.environment != 'prod' %}
        <!-- {{ _self }} => espt.css -->
        <link rel="stylesheet" href="{{ asset('css/espt.css', 'espt') }}">
    {% endif %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    {% if app.environment != 'prod' %}
        <!-- {{ _self }} => espt.js -->
        <script src="{{ asset('js/espt.js', 'espt') }}"></script>
    {% endif %}
{% endblock %}

